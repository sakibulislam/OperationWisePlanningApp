<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="StyleVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="mnj.mfg.model.lov.views.StyleVORowImpl"
  ComponentClass="mnj.mfg.model.lov.views.StyleVOImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.mfg.model.lov.views.StyleVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_buyerId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String"
    IsUpdateable="false">
    <TransientExpression><![CDATA[adf.context.sessionScope.orgId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT distinct mmph.STYLE_NAME_C style_name, mmph.SEASON_C season , mmph.BRAND_C BRAND,haou.ORGANIZATION_ID
          FROM CUST_MNJ_ONT_BOM mmph, ar_customers ac,
               hr_organization_units haou
         where ac.CUSTOMER_ID = mmph.buyer_id
           and mmph.STYLE_NAME_C is not null
           and haou.organization_id = NVL(mmph.ORG_ID,000)
           and haou.organization_id = :p_org_id
           and ac.CUSTOMER_ID = :p_buyerId
           AND NOT EXISTS (
                  SELECT * 
                    FROM MNJ_MFG_OSP_HEADER MMO
                   WHERE MMO.STYLE = mmph.STYLE_NAME_C
                     AND MMO.SEASON = mmph.SEASON_C 
                     AND MMO.ORG_ID = :p_org_id
                  )]]>
  </SQLQuery>
  <ViewAttribute
    Name="StyleName"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="STYLE_NAME"
    Expression="STYLE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Season"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="500"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEASON"
    Expression="SEASON"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Brand"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BRAND"
    Expression="BRAND"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="OrganizationId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="15"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ORGANIZATION_ID"
    Expression="ORGANIZATION_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="16"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.mfg.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
